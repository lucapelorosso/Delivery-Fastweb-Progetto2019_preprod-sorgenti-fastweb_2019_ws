            <WL5G3N2:operation style="document" soapAction="RetrieveCMPUsim"/>
            <WL5G3N0:input>
                <WL5G3N2:body use="literal"/>
            </WL5G3N0:input>
            <WL5G3N0:output>
                <WL5G3N2:body use="literal"/>
            </WL5G3N0:output>
        </WL5G3N0:operation>
    </WL5G3N0:binding>
    <WL5G3N0:service name="ManageCommercialInfoService">
        <WL5G3N0:documentation>OSB Service</WL5G3N0:documentation>
        <WL5G3N0:port name="ManageCommercialInfoPort" binding="WL5G3N1:ManageCommercialInfoBinding">
            <WL5G3N2:address location="http://ws302oag.se.intranet.fw:8080/Eudata/AdministrativeProcess/ManageCommercialInfo"/>
        </WL5G3N0:port>
    </WL5G3N0:service>
