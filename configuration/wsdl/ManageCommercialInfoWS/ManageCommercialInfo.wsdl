    </WL5G3N0:message>
    <WL5G3N0:message name="RetrieveCMPUsimResponse">
        <WL5G3N0:part name="RetrieveCMPUsimResponse" element="WL5G3N1:RetrieveCMPUsimResponse"/>
    </WL5G3N0:message>
    <WL5G3N0:portType name="ManageCommercialInfoPortType">
        <WL5G3N0:operation name="RetrieveCMPUsim">
            <WL5G3N0:input message="WL5G3N1:RetrieveCMPUsimRequest"/>
            <WL5G3N0:output message="WL5G3N1:RetrieveCMPUsimResponse"/>
        </WL5G3N0:operation>
    </WL5G3N0:portType>
    <WL5G3N0:binding name="ManageCommercialInfoBinding" type="WL5G3N1:ManageCommercialInfoPortType">
        <WL5G3N2:binding style="document" transport="http://schemas.xmlsoap.org/soap/http"/>
