						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="OnlinePaybilityStatus" minOccurs="1" maxOccurs="1" nillable="true">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="0"/>
								<xsd:maxLength value="255"/>
							</xsd:restriction>
						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="OnlinePayment" minOccurs="1" maxOccurs="1" nillable="true">
