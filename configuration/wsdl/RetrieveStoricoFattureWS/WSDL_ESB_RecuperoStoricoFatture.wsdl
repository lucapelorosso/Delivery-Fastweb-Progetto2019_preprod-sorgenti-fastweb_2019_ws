			<xsd:element name="RecuperoStoricoFattureRequest" type="tns:RecuperoStoricoFattureRequestType"/>
			<xsd:element name="RecuperoStoricoFattureResponse" type="tns:RecuperoStoricoFattureResponseType"/>
			<xsd:complexType name="RecuperoStoricoFattureRequestType">
				<xsd:sequence>
