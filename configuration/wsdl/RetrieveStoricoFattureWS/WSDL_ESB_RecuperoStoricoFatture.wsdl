						</xsd:simpleType>
					</xsd:element>
					<xsd:element name="DocTypeId" minOccurs="1" maxOccurs="1" nillable="true">
						<xsd:simpleType>
							<xsd:restriction base="xsd:string">
								<xsd:minLength value="0"/>
